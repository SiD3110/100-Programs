 #include<iostream>
 using namespace std;
 int main()
{   int n;
    cin>>n;
    int a[n];
    //for(int i=0;i<10;i++)num[i]=0;
    for(int i=0;i<n;i++)
	{
	  cin>>a[i];
    }
    int min=a[0],smin;
    for(int i=1;i<n;i++)
    {
	  if(a[i]<min)
	  {
	    smin=min;
	    min=a[i];
	  }
    }
    cout<<"Second Smallest:"<<smin;
  return 0;
} 
